package data.LrnCountryDeFrRo
default allow := false
allow if {
          input.credentialData.credentialSubject["gx:leiCode-countryCode"] == "DE"
         }

allow if {
          input.credentialData.credentialSubject["gx:leiCode-countryCode"] == "FR"
         }

allow if {
          input.credentialData.credentialSubject["gx:leiCode-countryCode"] == "RO"
         }
