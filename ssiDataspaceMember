package data.LrnCountryDeFrRo
default allow := false
allow if {
          input.credentialData.credentialSubject.dataspaceId == "DE"
         }

allow if {
          input.credentialData.credentialSubject.dataspaceId == "FR"
         }

allow if {
          input.credentialData.credentialSubject.dataspaceId == "RO"
         }
